{
  "name": "@mohirio/jupyterlab-horizon-theme",
  "version": "0.1.0",
  "description": "VSCode Horizon Theme port for JupyterLab",
  "author": "Mohammad Almogbel <almogbelmohi@gmail.com>",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "build:webpack": "webpack",
    "clean": "rimraf lib && rimraf static",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "~0.12.0",
    "typescript": "~3.1.1",
    "css-loader": "~0.28.7",
    "mini-css-extract-plugin": "~0.4.4",
    "svgo": "~1.0.4",
    "svg-url-loader": "~2.3.1",
    "svgo-loader": "~2.1.0",
    "url-loader": "~1.0.1",
    "watch": "~1.0.2",
    "webpack": "~4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themeDir": "style",
    "themePath": "style/index.css"
  },
  "homepage": "https://github.com/mohirio/jupyterlab-horizon-theme",
  "bugs": {
    "url": "https://github.com/mohirio/jupyterlab-horizon-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohirio/jupyterlab-horizon-theme.git"
  }
}
